	<s:Path x="0" y="1"
			data="M 6 10 L 15 0">
		<s:stroke>
			<s:SolidColorStroke color="#404040"
								weight="3"
								caps="round"
								pixelHinting="true"/>
		</s:stroke>
	</s:Path>
	
</s:Skin>