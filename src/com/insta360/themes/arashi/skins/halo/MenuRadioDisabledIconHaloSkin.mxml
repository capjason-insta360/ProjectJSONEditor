<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark"
		alpha="0.5">

	<s:Group width="15" height="15"
			 verticalCenter="0"
			 layoutDirection="ltr">
		
		<!--- border and fill -->
		<s:Ellipse top="0" right="0" bottom="0" left="0">
			<s:stroke>
				<s:SolidColorStroke pixelHinting="false" 
									color="#343434"/>
			</s:stroke>
			<s:fill>
				<s:LinearGradient rotation="90">
					<s:GradientEntry color="#676767"
									 ratio="0"/>
					<s:GradientEntry color="#676767"
									 ratio="1"/>
				</s:LinearGradient>
			</s:fill>
		</s:Ellipse>
		
		<!--- ellipse -->
		<s:Ellipse horizontalCenter="0" verticalCenter="0"
				   width="7" height="7"
				   alpha="1">
			<s:fill>
				<s:LinearGradient rotation="90">
					<s:GradientEntry color="#FFFFFF"
									 ratio="0"/>
					<s:GradientEntry color="#D7D7D7"
									 ratio="1" />
				</s:LinearGradient>
			</s:fill>
			<s:filters>
				<s:DropShadowFilter color="#00000"
									angle="90"
									distance="1"
									blurX="0" blurY="2"
									alpha="0.5" />
			</s:filters>
		</s:Ellipse>
		
	</s:Group>
	
</s:Skin>

